# API Configuration
VITE_API_BASE_URL=http://localhost:5002
VITE_APP_NAME=EthioAI Tourism Platform

# Authentication
VITE_TOKEN_KEY=auth_token

# Map Configuration
# Get your Mapbox access token from: https://account.mapbox.com/access-tokens/
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here
VITE_MAPBOX_STYLE=mapbox://styles/mapbox/streets-v12

# Payment Configuration (Public Keys Only)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
VITE_CHAPA_PUBLIC_KEY=your_chapa_public_key

# Feature Flags
VITE_ENABLE_CHAT=true
VITE_ENABLE_MARKETPLACE=true
VITE_ENABLE_TRANSPORT=true
VITE_ENABLE_CULTURAL=true
VITE_DEBUG_MODE=false

# Analytics (Public Keys Only)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Error Tracking (Public Keys Only)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true

# PWA Configuration
VITE_PWA_ENABLED=true
VITE_PWA_OFFLINE_ENABLED=true
